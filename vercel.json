{
    "version": 2,
    "builds": [
      {
        "src": "root/**",
        "use": "@vercel/static"
      },
      {
        "src": "public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "public/"
      },
      {
        "src": "/",
        "dest": "root/"
      }
    ]
  }